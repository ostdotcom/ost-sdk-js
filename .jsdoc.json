{
  "tags": {
    "allowUnknownTags": true
  },
  "source": {
    "include": ["lib/", "services/"],
    "exclude": [],
    "includePattern": ".+\\.js(doc|x)?$",
    "excludePattern": "(^|\\/|\\\\)_"
  },
  "plugins": [],
  "recurseDepth": 5,
  "templates": {
    "systemName"            : "OST Javascript SDK",
    "footer"                : "<style>.copyright, .jsdoc-message, footer { color: #555555;} footer { border-color:#eeeeee;} }</style>",
    "copyright"             : "Copyright 2018 OST.COM LTD.",
    "includeDate"           : false,
    "theme"                 : "lumen",
    "navType"               : "vertical",
    "linenums"              : true,
    "cleverLinks"           : true,
    "monospaceLinks"        : true,
    "default": {
      "outputSourceFiles": true
    }
  },
    "opts": {
    "destination": "./docs/",
    "encoding": "utf8",
    "private": true,
    "recurse": true,
    "template": "./node_modules/ink-docstrap/template",
    "readme": "./README.md"
  }
}
